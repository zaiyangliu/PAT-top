#include<iostream>
using namespace std;
int main(){
    string s;
    int  cnt;
    cin >> s >> cnt;
    for(int k = 1; k < cnt; ++k){
        string t;
        int j;
        for(int i = 0; i < s.length(); i = j){
            for(j = i; j < s.length() && s[j] == s[i]; ++j){
                
            }
            t += s[i] + to_string(j - i);
        }
        s = t;
    }
    cout << s;
    return 0;
}
